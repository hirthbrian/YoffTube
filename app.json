{
  "expo": {
    "name": "YoffTube",
    "description": "A YouTube video downloader",
    "slug": "yofftube",
    "privacy": "public",
    "githubUrl": "https://github.com/hirthbrian/YoffTube",
    "sdkVersion": "31.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "primaryColor": "#FF5B5A",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "backgroundColor": "#FFFFFF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.brian.yofftube"
    },
    "android": {
      "package": "com.brian.yofftube"
    }
  }
}
